{
  "name": "webpack-starter",
  "version": "0.0.2",
  "description": "webpack starter",
  "main": "build/main.js",
  "engine": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "npm run test && npm run build:clean",
    "build:clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --color -p",
    "start": "node devServer.js",
    "lint": "eslint src",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jimexist/webpack-starter.git"
  },
  "keywords": [
    "webpack",
    "starter"
  ],
  "author": "Jiayu Liu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jimexist/webpack-starter/issues"
  },
  "homepage": "https://github.com/Jimexist/webpack-starter#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "cssnano": "^3.6.2",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.1",
    "eslint-plugin-react": "^5.1.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "lost": "^6.8.0",
    "mocha": "^2.4.5",
    "postcss": "^5.0.21",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
