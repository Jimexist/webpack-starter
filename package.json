    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "normalize.css": "^4.1.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.10.4",
    "reselect": "^2.5.1"
  }
